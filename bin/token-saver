#!/usr/bin/env python3
"""Token-Saver CLI wrapper. Resolves symlinks to find the repo directory."""

import os
import sys

# Resolve symlinks to find the real repo location
script_path = os.path.realpath(__file__)
repo_dir = os.path.dirname(os.path.dirname(script_path))
sys.path.insert(0, repo_dir)

from src.cli import main

main()
